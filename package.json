{
  "name": "loguno",
  "version": "2.0.0",
  "description": "One logger, many outlets",
  "keywords": [
    "logger",
    "loglevel",
    "winston",
    "debug"
  ],
  "author": "Henry Aguila",
  "license": "MIT",
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/user?u=80437116"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hankaguila/loguno"
  },
  "homepage": "https://github.com/hankaguila/loguno#readme",
  "bugs": "https://github.com/hankaguila/loguno/issues",
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "node": "node --experimental-specifier-resolution=node --no-warnings",
    "test": "vitest --dir=test run",
    "lint": "eslint src --fix",
    "dist:cjs": "tsup src/index.ts --format=cjs",
    "dist:esm": "tsup src/index.ts --format=esm && shx mv dist/index.js dist/index.mjs",
    "dist:dts": "tsup src/index.ts --dts-only --format=esm",
    "dist": "shx rm -rf dist && npm run dist:cjs && npm run dist:esm && npm run dist:dts"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/parser": "^5.48.0",
    "documentation": "^14.0.2",
    "eslint": "8.22.0",
    "shx": "^0.3.4",
    "tsup": "^7.1.0",
    "typescript": "^4.9.5",
    "vitest": "^0.32.0"
  },
  "dependencies": {
    "cstack": "^0.3.2"
  }
}
